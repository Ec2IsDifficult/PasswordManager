      <Label layoutX="226.0" layoutY="142.0" text="Type master password" />
      <Button fx:id="loginButton" layoutX="226.0" layoutY="188.0" mnemonicParsing="false" onAction="#loginAction" text="Login" />
      <Label fx:id="feedbackLabel" layoutX="375.0" layoutY="163.0" />
